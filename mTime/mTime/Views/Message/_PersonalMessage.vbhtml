@Code
    ViewData("Title") = "_PersonalMessage"
End Code

<div class="message_lt_part">
    <div class="inbox_haedtext">
        <span>Personal Message</span>
    </div>

    <div class="overflow_box">
        <table class="epy_lt_table">
            <thead>
                <tr>
                    <th style="width: 60px;"></th>
                    <th style="width: 140px;">Date</th>
                    <th style="width: 200px;">Employee</th>
                    <th>Title</th>
                </tr>
            </thead>

            <tbody>
                <tr>
                    <td><div class="ball_color1"></div></td>
                    <td>23 May 2020 16:00:00</td>
                    <td>HAMID NAME</td>
                    <td>Changes of Shift Schedule</td>
                </tr>
            </tbody>
        </table>
    </div>


    <div class="footer_row_total">
        <span>Total : 1 row</span>
    </div>
</div>

<div class="message_rt_part">
    <div class="inbox_haedtext">
        <span>Click "Create" button to create a new Personal Message and send to employee</span>
    </div>

    <div class="overflow_box" style="height: 435px;">

    </div>

    <div class="footer_row_btn" style="margin: 0 0 10px 0;">
        <a href="" id="save" class="rtpt_savebtn filter1">Send</a>
        <a href="" id="save" class="rtpt_yesbtn filter1">Create</a>
        <a href="" id="save" class="rtpt_deletebtn filter1">Delete</a>

    </div>
</div>
